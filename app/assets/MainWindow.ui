	border: 1px solid #95d5b2;
	border-radius: 33px;
}

/* --- Base Style for QPushButton (Always applied) --- */
QPushButton {
	/* New base styles provided by the user */
	border: 2px solid #95d5b2;
	border-radius: 10px;
	color: #85be9f; /* A light green text color */
	padding: 8px 16px; /* Added padding for a better button size */
	background-color: white;
	font-weight: bold;
	/* Add a smooth transition for visual effect */
	transition: all 0.2s ease-in-out; 
}

/* --- Hover State (When the mouse is over the button) --- */
QPushButton:hover {
	/* Slightly darken the background to show interaction */
	background-color: #f1f7f0; 
	/* Change the border color to a slightly darker shade */
	border: 2px solid #6d9f86; 
	/* Darken the text color for contrast */
	color: #6d9f86;
	/* Optional: Lift the button slightly using a subtle shadow */
	box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
}

/* --- Pressed State (When the button is clicked down) --- */
QPushButton:pressed {
	/* Invert the colors or make them significantly darker */
	background-color: #6d9f86; /* Darker background */
	border-color: #557d6b;
	color: white; /* White text for contrast */
	/* Optional: Remove the shadow to give a &quot;pushed in&quot; effect */
	box-shadow: none;
}


/* --- Checked State (For toggle buttons, when the button is active/toggled) --- */
QPushButton:checked {
	/* Primary Active/Toggled Look */
	background-color: #85be9f; /* Match the text color for a filled look */
	border: 2px solid #6d9f86;
	color: white;
}</string>
