                                 <property name="styleSheet">
                                  <string notr="true">/* --- General Calendar Widget --- */
QCalendarWidget 
{
    background-color: #2b2b2b;
    border: 1px solid #444444;
    font-size: 14px;
}

/* --- Navigation Bar (Month/Year and Arrows) --- */
QCalendarWidget QWidget#qt_calendar_navigationbar 
{ 
    background-color: #3c3c3c; 
    border-bottom: 1px solid #444444;
}

QCalendarWidget QToolButton 
{
    color: #DDDDDD; 
    background-color: #3c3c3c; 
    border: none;
    font-size: 14px;
    padding: 5px;
}
QCalendarWidget QToolButton:hover { background-color: #505050; }
QCalendarWidget QToolButton:pressed { background-color: #606060; }

/* Fix for the year/month spinbox text color */
QCalendarWidget QSpinBox {
    color: #FFFFFF;
    background-color: #3c3c3c;
    border: 1px solid #555555;
}

/* --- Month Dropdown List (Fix for Image 1) --- */
QCalendarWidget QMenu 
{
    background-color: #3c3c3c; /* Background of the dropdown list */
    border: 1px solid #555555;
}

QCalendarWidget QMenu::item 
{
    color: #FFFFFF; /* Text color of months in the list (The fix!) */
    padding: 3px 20px 3px 20px;
}

QCalendarWidget QMenu::item:selected 
{
    background-color: #0078D7;
    color: #FFFFFF;
}


/* --- Day Grid and Headers (Fix for Image 2) --- */
QCalendarWidget QTableView 
{
    gridline-color: #444444; 
    background-color: #2b2b2b;
}

/* Weekday Headers (Sun, Mon, Tue...): Fixes their text color */
QCalendarWidget QTableView::item:!enabled 
{
    color: #CCCCCC; /* Light grey for most weekday headers */
    background-color: #2b2b2b;
}

/* Weekend Headers */
QCalendarWidget QTableView::item:!enabled:first-column, 
QCalendarWidget QTableView::item:!enabled:last-column    
{
    color: #FF7070; /* Brighter red for weekend headers */
}

/* Day Numbers (1, 2, 3...): Fixes their text color */
QCalendarWidget QAbstractItemView:enabled 
{
    color: #FFFFFF; 
    background-color: #2b2b2b;
    selection-background-color: #0078D7;
    selection-color: #FFFFFF;
}

/* Specific colors for weekend day numbers */
QCalendarWidget QAbstractItemView:enabled:first-column { color: #FF6060; } /* Sundays */
QCalendarWidget QAbstractItemView:enabled:last-column { color: #FF6060; } /* Saturdays */
QCalendarWidget QAbstractItemView:disabled { color: #888888; } /* Dates outside current month */</string>
