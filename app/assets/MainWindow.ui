   HORIZONTAL SCROLLBAR STYLES (QScrollBar:horizontal)
   ========================================================================= */

QScrollArea QScrollBar:horizontal {
    /* 1. Overall Scrollbar Container */
    border: none;
    background: transparent;
    height: 8px; /* Height matches the vertical width */
    margin: 0px;
}

QScrollArea QScrollBar::groove:horizontal {
    /* 2. Scrollbar Track (Groove) */
    border: none;
    border-radius: 4px;
    background: rgba(0, 0, 0, 10);
}

QScrollArea QScrollBar::handle:horizontal {
    /* 3. Draggable Handle (Thumb) - Default State */
    background: #2aab7b;
    min-width: 25px; /* Ensure the handle is easy to grab */
    border-radius: 4px;
}

QScrollArea QScrollBar::handle:horizontal:hover {
    /* 4. Draggable Handle - Hover State */
    background: #404040;
}

QScrollArea QScrollBar::add-line:horizontal, QScrollArea QScrollBar::sub-line:horizontal {
    /* 5. Remove Arrow Buttons */
    border: none;
    background: none;
    width: 0px;
}


